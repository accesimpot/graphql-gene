{
  "private": true,
  "name": "@graphql-gene/plugin-sequelize",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development pnpm build --watch --emptyOutDir=false",
    "build": "NODE_ENV=${NODE_ENV:-development} vite build",
    "types:check": "tsc --project tsconfig.lint.json"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "graphql-lookahead": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "concurrently": "^8.2.2",
    "dev-utils": "workspace:*",
    "graphql": "^16.9.0",
    "graphql-gene": "workspace:*",
    "nodemon": "^3.1.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "typescript": "^5.6.2",
    "vite": "^5.4.1"
  }
}
